{
  "name": "navbar-tailwind",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "test-format": "npm run format && next lint",
    "husky": "npx husky init",
    "pre-commit": "npm run format && next lint && git add -A .",
    "pre-commit2": "npm run format && next lint && npm run build && git add -A .",
    "husky:mac": "chmod ug+x .husky/*",
    "prepare": "husky"
  },
  "dependencies": {
    "husky": "^9.0.11",
    "next": "14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
